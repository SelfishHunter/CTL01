package interest_calculator;

import org.junit.jupiter.api.DisplayName;
import org.junit.jupiter.api.Test;
class Junit5 {

	@Test
	@DisplayName("Simple Interest")

	void test1() {
		I_calculator ic = new I_calculator();
	ic.setPrinciple(1000);
	ic.setTime(5);
		ic.setRate(100);
		double SI = ic.SI();
		System.out.println("Simple Interest is "+SI);
	}

	@Test
	@DisplayName("Simple Interest")

	void test2() {
		I_calculator ic = new I_calculator();
	ic.setPrinciple(1000);
	ic.setTime(5);
		ic.setRate(100);
		double SI = ic.SI();
		System.out.println("Simple Interest is "+SI);
	}
}
